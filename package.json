{
  "name": "rxjs-channel",
  "version": "0.2.0",
  "description": "CSP channels for RxJS",
  "keywords": [
    "rxjs",
    "rx",
    "csp",
    "channel"
  ],
  "scripts": {
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --print-width 80 --tab-width 2 --no-semi --single-quote --trailing-comma=es5",
      "eslint --fix",
      "git add"
    ]
  },
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alex-shnayder/rxjs-channel.git"
  },
  "author": "Alex Shnayder <alex@shnayder.pro>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alex-shnayder/rxjs-channel/issues"
  },
  "homepage": "https://github.com/alex-shnayder/rxjs-channel#readme",
  "peerDependencies": {
    "rxjs": "^5.0.0"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-prettier": "^2.0.1",
    "husky": "^0.13.3",
    "lint-staged": "^3.4.0",
    "prettier": "^1.2.2"
  }
}
